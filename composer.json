{
    "name": "yiisoft/yii-captcha",
    "type": "library",
    "description": "Yii Framework Captcha widget Extension",
    "keywords": ["yii", "captcha", "turing test"],
    "license": "BSD-3-Clause",
    "support": {
        "source": "https://github.com/yiisoft/yii-captcha",
        "issues": "https://github.com/yiisoft/yii-captcha/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii"
    },
    "authors": [
        {
            "name": "Qiang Xue",
            "email": "qiang.xue@gmail.com"
        },
        {
            "name": "Alexander Makarov",
            "email": "sam@rmcreative.ru",
            "homepage": "http://rmcreative.ru/"
        },
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "yiisoft/core": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/log": "^3.0@dev",
        "yiisoft/cache": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {"yii\\captcha\\": "src"}
    },
    "autoload-dev": {
        "psr-4": {"yiiunit\\captcha\\": "tests"}
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
